{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    // "baseUrl": "./src", // 将src目录作为根目录
    "rootDir": "src", // 基础路径
    "module": "ES2022",
    "moduleResolution": "Bundler", // 生成声明文件
    // "strict": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
